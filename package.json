{
  "name": "lovelab-api",
  "version": "0.1.0",
  "description": "API server of lovelab with Express.js",
  "engines": {
    "node": "12.x"
  },
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc --build tsconfig.json",
    "server": "node dist/index.js",
    "server:reset": "node dist/index.js --reset",
    "start": "tsc && node dist/index.js",
    "start:reset": "tsc && node dist/index.js --reset",
    "lint": "eslint src --ext .ts",
    "fix": "eslint --fix src --ext .ts",
    "doc": "tsc && node dist/others/errorTest.js > documents/errorMessages.md",
    "test": "jest --forceExit",
    "test:coverage": "jest --forceExit --coverage"
  },
  "dependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/pg": "^7.11.2",
    "@types/sequelize": "^4.28.7",
    "@types/sha256": "^0.2.0",
    "@types/supertest": "^2.0.8",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "pg": "^7.12.1",
    "sequelize": "^5.21.2",
    "sha256": "^0.2.0",
    "supertest": "^4.0.2",
    "typescript": "^3.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/basd4g/lovelab-api.git"
  },
  "keywords": [
    "node",
    "express"
  ],
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix src --ext .ts",
      "git add"
    ]
  }
}
